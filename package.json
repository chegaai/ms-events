{
  "name": "ms-events",
  "version": "0.0.1",
  "description": "Events microservice",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build:clean",
    "clean": "rm -rf ./dist",
    "test": "exit 0",
    "coverage": "exit 0",
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "build:debug": "npm run clean && npm run build -- --sourceMap",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "start:debug": "tsc && concurrently -s all \"tsc -w\" \"DEBUG=expresso:* nodemon dist/index.js\""
  },
  "contributors": [
    "Lucas Santos <hello@lsantos.dev>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@expresso/app": "^1.0.7",
    "@expresso/errors": "^1.2.0",
    "@expresso/server": "^1.1.3",
    "@expresso/validator": "^1.0.4",
    "@nindoo/mongodb-data-layer": "^1.2.0",
    "express-rescue": "^1.1.22",
    "reflect-metadata": "^0.1.13",
    "sugar-env": "^1.5.7",
    "tsyringe": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.5",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  }
}
